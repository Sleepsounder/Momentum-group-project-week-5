<h1>PHIGGit</h1>
<h3>Story Listings</h3>

<p><%= link_to 'Add a Story', new_story_path, class: "button" %></p>

<% @stories.each do |story| %>
<div class="phigg_container">
    <div class="phigg_header"><%= story.title %></div>
    <div class="phigg_body"></div>
    <div class="phigg_footer">Created by: <%= story.user_id %> on <%= story.timestamp %>. <%# vote_count here somehow? %></div>
</div>
<% end %>


